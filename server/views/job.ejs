<script src="/socket.io/socket.io.js"></script>
<script>
  var socket = io("http://localhost:12345");
  var jobId = "<%= jobId %>";
  	console.log("Timeout");
  	socket.emit('joinJob', {jobId:jobId});
  	socket.on('clientConnect', function(msg){
  		console.log("Got connect event");
  		console.log(msg);
  	});

  	socket.on('clientDiconnect', function(msg){
  		console.log("Got disconnect event");
  		console.log(msg);
  	})
</script>