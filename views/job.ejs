<script src="/parallel.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/1.3.3/socket.io.js"></script>
<script>
  var socket = io("http://powerful-headland-8594.herokuapp.com");
  var jobId = "<%= jobId %>";
  	console.log("Timeout");
  	socket.emit('joinJob', {jobId:jobId, role: "worker"});
  	socket.on('clientConnect', function(msg){
  		console.log("Got connect event");
  		console.log(msg);
  	});

  	socket.on('clientDisconnect', function(msg){
  		console.log("Got disconnect event");
  		console.log(msg);
  	})
</script>
<script src="/JobRunner.js"> </script>
